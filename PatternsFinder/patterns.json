{
    "email": {
        "label": "email",
        "description": "Check and detect emails from text.",
        "sources": [
            "https://regexr.com/3e48o"
        ],
        "regex": "(?<=\\s|^)\\w+([-+.']\\w+)*@\\w+([-.]\\w+)*\\.\\w+([-.]\\w+)*(?=\\s|$|\\.)",
        "matches": [
            "h@a.co",
            "me@con.com",
            "not-re_aly@gmail.com",
            "him.him@that.this"
        ],
        "non-matches": [
            "test@test@s.com",
            "@site.com",
            "test-@not-b.com"
        ],
        "tages": [
            "email",
            "mail",
            "web"
        ]
    },
    "url": {
        "label": "url",
        "description": "Check and detect URLs from text.",
        "sources": [
            "https://stackoverflow.com/posts/17773849/revisions",
            "https://stackoverflow.com/questions/3809401/what-is-a-good-regular-expression-to-match-a-url"
        ],
        "regex": "((ftp|file|https?):\\/\\/(?:www\\.|(?!www))[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|www\\.[a-zA-Z0-9][a-zA-Z0-9-]+[a-zA-Z0-9]\\.[^\\s]{2,}|https?:\\/\\/(?:www\\.|(?!www))[a-zA-Z0-9]+\\.[^\\s]{2,}|www\\.[a-zA-Z0-9]+\\.[^\\s]{2,})",
        "matches": [
            "https://www.example.com/questions/3456/my-document",
            "ftp://www.example.com/questions/3456/my-document.txt",
            "www.example.com",
            "example.com/questions/3456/my-document"
        ],
        "non-matches": [
            "ht://www.example.com/questions/3456/my-document",
            "/example/questions/3456/my-document.tx",
            "example-com/questions/3456/my-document"
        ],
        "tages": [
            "url",
            "web",
            "protocol",
            "http",
            "link",
            "uri"
        ]
    },
